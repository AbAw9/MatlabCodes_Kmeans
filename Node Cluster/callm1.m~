clc
Data = xlsread('M1.xls');
[Adj n]= getAdj(Data);
SM = sparse(Adj);
s=sum(sum(Adj));

c1=[1 2 3 4 5 6];c2=[7 17 18 19 20 21 22 23 24 32];c3=[8 9 10 11 12 13 14 15 16];c4=[30 25 26 27 28 29 31];

C1(c1)=1;C1([c2 c3 c4])=2; 

C2([c1 c2])=1; C2([c3 c4])=2;

C3([c1 c2 c3])=1; C3(c4)=2;

C4([c1 c3])=1; C4([c2 c4])=2;

C5([c1 c4])=1; C5([c2 c3]) =2; 

C6([c1 c3 c4])=1; C6(c2)=2;

C7([c1 c2 c4])=1; C7(c3)=2;

C8(c1)=1; C8(c2)=2; C8(c3)=3; C8(c4)=4;

[modularity(SM,C1,n,s)
modularity(SM,C2,n,s)
modularity(SM,C3,n,s)
modularity(SM,C4,n,s)
modularity(SM,C5,n,s)
modularity(SM,C6,n,s)
modularity(SM,C7,n,s)
modularity(SM,C8,n,s)]